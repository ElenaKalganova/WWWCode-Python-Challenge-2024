   "source": [
    "from numpy import random"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "68b7a59a-6f29-4c8d-9c87-5cfbe3c74e90",
   "metadata": {},
   "outputs": [],
   "source": [
    "def largest_number(n1,n2,n3):\n",
    "    largest=max(n1,n2,n3)\n",
    "    return(largest)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
