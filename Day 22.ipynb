   "source": [
    "Create a program to find the second-largest element in a list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "3fc5403e-dfbe-4636-b135-3ee25e962425",
   "metadata": {},
   "outputs": [],
   "source": [
    "def second_largest(l):\n",
    "    l_sorted=l;\n",
    "    l_sorted.sort()\n",
    "    return(l_sorted[-2])\n",
    "    "
   ]
